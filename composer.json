{
  "name": "eddmash/powerorm",
  "type": "library",
  "keywords": [
    "orm",
    "model",
    "query",
    "php",
    "database",
    "dbal",
    "django",
    "migrations"
  ],
  "description": "A powerful php orm with automatic migrations",
  "homepage": "https://github.com/eddmash/powerorm",
  "license": "MIT",
  "authors": [
    {
      "name": "EDDILBERT MACHARIA",
      "email": "edd.cowan@gmail.com",
      "homepage": "http://eddmash.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/dbal": "^v2.5",
    "symfony/console": "~2.8|~3.1",
    "symfony/polyfill-mbstring": "~1.0",
    "symfony/polyfill-php71": "^1.3",
    "symfony/event-dispatcher": "^3.3",
    "doctrine/collections": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~5",
    "symfony/debug": "^2.8 | ^3.1",
    "symfony/var-dumper": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Eddmash\\PowerOrm\\": "src/",
      "Eddmash\\PowerOrm\\Tests\\": "tests/"
    },
    "files": ["src/Model/Query/Expression/Util.php"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "suggest": {
    "eddmash/powerormfaker": "powerormfake generates dummy data when using PowerOrm. An extension of Faker library",
    "eddmash/powerform": "makes it easy to create and process forms and easily integrate with powerorm models"
  },
  "support": {
    "issues": "https://github.com/eddmash/powerorm/issues"
  }
}
